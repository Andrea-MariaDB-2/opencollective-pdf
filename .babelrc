{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": "auto"
        },
        "transform-runtime": {
          "useESModules": false
        }
      }
    ]
  ],
  "plugins": [
    "lodash",
    [
      "react-intl",
      {
        "messagesDir": "./dist/messages/"
      }
    ]
  ],
  "env": {
    "development": {
      "compact": false
    }
  }
}
